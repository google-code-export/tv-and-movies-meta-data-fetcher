<script type="text/javascript">
TV.init.include(function(){
    // grab the nav so we can add events.
    // grab the conten_wrap so we can move the nav into it
    var global_nav = $('global_nav'), content_wrap = $('content_wrap');
    if(!global_nav && !content_wrap){return;}

    // grab the sub nav from the global_nav
    var sub_nav = global_nav.getElements('ul.subnav');
    if(!sub_nav || sub_nav.length < 0){return;}

    // move nav
    global_nav.addClass('js_on').inject(content_wrap);
    var global_nav_coords = global_nav.getCoordinates();

    if(Browser.Engine.trident4 || (Browser.Engine.gecko && Browser.Platform.mac)){
        var iframe_shim = new Element('iframe', {
            'id': 'global_nav_shim',
            'scrolling': 'no',
            'frameborder': 0,
            'styles': {
                'width': global_nav_coords.width,
                'filter': 'progid:DXImageTransform.Microsoft.Alpha(style=0,opacity=0)'
            }
        }).inject(global_nav, 'after');
    }

    // create an iframeshim for each and attach an event to the parent of each individual nav
    sub_nav.each(function(nav, i){

        nav.getParent().addEvents({
            'mouseover': function(){
                nav.addClass('on').setStyle('display', 'block');
                if($chk(iframe_shim)){
                    iframe_shim.addClass('on').setStyles({
                        'left': nav.getCoordinates(global_nav).left,
                        'height': nav.getCoordinates().height,
                        'width': nav.getCoordinates().width
                    });
                }
            },
            'mouseout': function(){
                nav.removeClass('on').setStyle('display', 'none');
                if($chk(iframe_shim)){
                    iframe_shim.removeClass('on');
                }
            }
        });
    });
});
</script>

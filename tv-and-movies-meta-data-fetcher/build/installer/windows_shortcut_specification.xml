<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
  <skipIfNotSupported/>
  <programGroup defaultName="Media Manager" location="applications"/>
  <shortcut
    name="Media Manager CLI Documentation (HTML)"    
    target="$INSTALL_PATH|docs|html|index.html"       
    description="The command line documentation in HTML format"        
    programGroup="yes"
    desktop="no"
    applications="yes"
    startMenu="no"
    startup="no">    
  </shortcut>
  
  <shortcut
    name="Media Manager CLI Documentation (PDF)"    
    target="$INSTALL_PATH|docs|pdf|userguide.pdf"       
    description="The command line documentation in HTML format"        
    programGroup="yes"
    desktop="no"
    applications="yes"
    startMenu="no"
    startup="no">    
  </shortcut>
  
  <shortcut
     name="Media Manager Uninstall"     
     programGroup="yes"
     desktop="no"
     applications="yes"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH|bin|mm-java.bat"
     commandLine="-jar &quot;$INSTALL_PATH|Uninstall|remove.task&quot;"          
     type="Application"
     description="Uninstaller">
   </shortcut> 
</shortcuts>